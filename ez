
local UILib = loadstring(game:HttpGet("https://github.com/w1201s/Yes-ui/raw/refs/heads/main/uno%20ui"))()

local Window = Library:CreateWindow("My UI")

-- Tab 1: Toggles
local Toggles = Window:AddTab("Toggles")

Toggles:AddToggle("Fly", false, function(on)
    print("Fly is", on and "ON" or "OFF")
end)

-- Tab 2: Buttons
local Buttons = Window:AddTab("Buttons")

Buttons:AddButton("Say Hi", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0,50,0)
    print("Teleported!")
end)
