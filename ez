
local UILib = loadstring(game:HttpGet("https://github.com/w1201s/Yes-ui/raw/refs/heads/main/uno%20ui"))()

local Window = UI_LIB:CreateWindow("My Cool UI")

local MainTab = Window:AddTab("Main")
MainTab:AddButton("Hello World", function()
    print("Hello from button!")
end)

MainTab:AddToggle("Speed Boost", false, function(state)
    print("Speed boost:", state)
end)

local SettingsTab = Window:AddTab("Settings")
SettingsTab:AddButton("Reset", function()
    print("Reset clicked!")
end)
